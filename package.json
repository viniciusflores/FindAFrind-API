    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "18.15.11",
    "@types/supertest": "2.0.12",
    "@vitest/coverage-c8": "0.30.1",
    "@vitest/ui": "0.30.1",
    "eslint": "^8.0.1",
    "npm-run-all": "4.1.5",
    "supertest": "6.3.3",
    "tsup": "6.7.0",
    "tsx": "3.12.6",
    "typescript": "5.0.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.30.1"
